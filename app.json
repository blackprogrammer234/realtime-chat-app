{
  "expo": {
    "name": "realtime-chat-app",
    "slug": "realtime-chat-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/realtime_chat_icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Social media app similiar to whatsapp. Users are able to create chat room so that they can instant message other people that are using the app including family and friends."
  }
}
